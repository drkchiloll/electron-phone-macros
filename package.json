{
  "name": "imperium-phone-controller",
  "version": "1.0.0",
  "description": "A Standalone Application for the removal of ITL Certs on Cisco IP Phones",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "rebuild": "node_modules/.bin/electron-rebuild -p -t \"dev,prod,optional\"",
    "postinst_java": "node node_modules/java/postInstall.js",
    "build_java": "npm run rebuild && npm run postinst_java",
    "build_java_win": "npm run rebuild_java_win && npm run postinst_java",
    "rebuild_java_win": "node_modules/.bin/electron-rebuild.cmd -p -t \"dev,prod,optional\"",
    "build_robotjs": "node-gyp rebuild --runtime=electron --target=2.0.4 --disturl=https://atom.io/download/atom-shell",
    "build:dev": "webpack --mode development --watch --profile",
    "build:dist": "node_modules/.bin/webpack --config webpack.config.js --mode production --progress --colors",
    "start": "node_modules/.bin/electron .",
    "package:macos": "node_modules/.bin/electron-packager ./ Imperium_Control --platform=darwin --arch=x64 --prune=true --ignore=src/app --ignore=node_modules/@types --ignore=node_modules/.cache --ignore=node_modules/electron* --app-version=4.0.0 --icon ./resources/icon.icns",
    "package:win64": "node_modules/.bin/electron-packager ./ imperium --platform=win32 --arch=x64 --prune=true --ignore=src/app --ignore=node_modules/@types --ignore=node_modules/.cache --ignore=node_modules/electron* --app-version=4.0.0 --icon ./resources/icon.ico",
    "clean": "cd dist && rm *.js* && cd ..",
    "clean_jtapi": "rm CiscoJtapi*"
  },
  "dependencies": {
    "java": "^0.10.0",
    "robotjs": "^0.5.1",
    "targz": "^1.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/jquery": "^2.0.41",
    "@types/material-ui": "^0.20.8",
    "@types/moment": "^2.13.0",
    "@types/nedb": "^1.8.3",
    "@types/node": "^7.0.13",
    "@types/react": "^15.0.14",
    "@types/react-beautiful-dnd": "^7.1.0",
    "@types/react-dom": "^0.14.23",
    "@types/winston": "^2.3.9",
    "@types/xmldom": "^0.1.29",
    "awesome-typescript-loader": "^5.1.1",
    "axios": "^0.18.0",
    "css-loader": "^0.26.1",
    "cucm-risdevice-query": "^1.1.1",
    "docxtemplater": "^3.8.1",
    "electron": "^2.0.4",
    "electron-builder": "^20.31.2",
    "electron-context-menu": "^0.10.0",
    "electron-packager": "^8.5.2",
    "electron-rebuild": "^1.7.3",
    "file-loader": "^0.10.1",
    "firebase": "^5.3.1",
    "font-awesome": "^4.7.0",
    "image-size": "^0.6.3",
    "jquery": "^3.1.1",
    "json2csv": "^4.2.0",
    "jszip": "^2.6.1",
    "material-ui": "^0.20.1",
    "moment": "^2.22.2",
    "nan": "^2.10.0",
    "nedb-core": "^3.0.6",
    "node-gyp": "^3.7.0",
    "node-loader": "^0.6.0",
    "open-docxtemplater-image-module": "^1.0.3",
    "react": "^16.2.0",
    "react-beautiful-dnd": "^7.1.3",
    "react-dom": "^16.2.0",
    "react-styled-flexboxgrid": "^2.4.0",
    "react-toggle-button": "^2.2.0",
    "style-loader": "^0.13.1",
    "styled-components": "^3.3.3",
    "sudo-prompt": "^8.2.3",
    "ts-loader": "^0.8.2",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^0.5.6",
    "webexteams-guestissuer": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-merge": "^3.0.0",
    "winston": "^3.0.0",
    "xmldom": "^0.1.27",
    "xpath": "0.0.27"
  }
}
